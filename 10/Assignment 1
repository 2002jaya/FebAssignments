sname=input ('Enter student name: ')

sgroup=input ('Enter student group: ')

s1=int(input ('Enter accounts marks: '))

s2=int(input ('Enter java marks: '))

s3=int(input ('Enter OS marks: '))

s4=int(input ('Enter Telugu marks: '))

s5=int(input ('Enter English marks: '))

s6=int(input ('Enter Analytical skills marks: '))

if s1>=35 and s2>=35 and s3>=35 and s4>=35 and s5>=35 and s6>=35:

      print (' Pass')

else:

     print ('Fail')

