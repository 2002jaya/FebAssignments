def fun(x):

    k=[]

    for num in x:

        if  num<= 1:

            continue

        for i in range(2,num):

            if num%i==0:
        

                break

        else:

            

            k.append(num)

    return(len(k))

n=int(input())

z=[]

for i in range(n):

    x=int(input())

    z.append(x)

print(fun(z))

